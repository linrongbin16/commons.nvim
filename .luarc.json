{
  "diagnostics.globals": [
    "vim",
    "describe",
    "before_each",
    "it",
    "jit",
    "utf8"
  ],
  "diagnostics.disable": [
    "undefined-field",
    "inject-field",
    "deprecated",
    "luadoc-miss-module-name",
    "undefined-doc-name",
    "lowercase-global",
    "cast-local-type",
    "param-type-mismatch"
  ],
  "runtime.version": "LuaJIT",
  "workspace.checkThirdParty": "Disable"
}
